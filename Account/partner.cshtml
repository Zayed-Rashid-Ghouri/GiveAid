@model IEnumerable<Give_Aid.partner>

@{
    ViewBag.Title = "partner";
    Layout = "~/Views/Shared/layout_backend.cshtml";
}

<h2>partner</h2>

<p>
    @Html.ActionLink("Create New", "partner_createnew")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.img)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.img)
        </td>
        <td>
            @Html.ActionLink("Edit", "edit_partner", new { id=item.id }) |

            @Html.ActionLink("Delete", "Delete_partner", new { id=item.id })
        </td>
    </tr>
}

</table>
